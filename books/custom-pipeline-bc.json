{
  "kind": "BuildConfig",
  "apiVersion": "build.openshift.io/v1",
  "metadata": {
    "name": "custom-pipeline"
  },
  "spec": {
    "source": {
      "type": "Git",
      "git": {
        "url": "https://github.com/javapark1/DO288-apps.git",
        "ref": "customer-pipeline"
      },
      "contextDir": "books"
    },
    "strategy": {
      "type": "JenkinsPipeline",
      "jenkinsPipelineStrategy": {
        "jenkinsfilePath": "jenkins/Jenkinsfile"
      }
    }
  }
}
